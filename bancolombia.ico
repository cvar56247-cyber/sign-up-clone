    <script>

    let funcionalidadesBtnDataLayer = () => {


        function limpiarTexto(texto) {
            // Eliminar tildes y convertir a minúsculas
            const textoSinTildes = texto.normalize('NFD').replace(/[\u0300-\u036f]/g, '');
            // Eliminar caracteres especiales
            const textoLimpio = textoSinTildes.replace(/[^a-z0-9áéíóúüñ\s]/gi, '').toLowerCase();
            return textoLimpio;
        }


        // Cards
        if (document.querySelectorAll(".wcmbc-rotator-banner-destacado .botombanner a").length >= 1) {

            document.querySelectorAll(".wcmbc-rotator-banner-destacado .botombanner a")?.forEach($btn => {


                if (!$btn.classList.contains("event-data-layer")) {
                    $btn.classList.add("event-data-layer");

                    $btn.addEventListener("click", () => {
                        dataLayer.push({
                            event: "SEND_CARDS_BANCOLOMBIA",
                            seccion: "personas",
                            activo: "bancolombia",
                            nombreFlujo: "home",
                            elemento: $btn.dataset.nameboton ? limpiarTexto($btn.dataset.nameboton) : "", // ejemplo: "abre tu cuenta",
                            ubicacion: "banner principal"
                        })
                    })
                }



            });

        }

        // Enlaces de los Banner
        if (document.querySelectorAll(".wcmbc-rotator-banner-destacado .banner-destacado-texto a").length >= 1) {

            document.querySelectorAll(".wcmbc-rotator-banner-destacado .banner-destacado-texto a")?.forEach($btn => {


                if (!$btn.classList.contains("event-data-layer")) {
                    $btn.classList.add("event-data-layer");

                    $btn.addEventListener("click", () => {
                        dataLayer.push({
                            event: "SEND_BOTONES_BANCOLOMBIA",
                            seccion: "personas",
                            activo: "bancolombia",
                            nombreFlujo: "home",
                            opcionSeleccionada: $btn.dataset.card ? limpiarTexto($btn.dataset.card) : "", // ejemplo: "abre tu cuenta de ahorros desde 0",
                            elemento: $btn.dataset.nameboton ? limpiarTexto($btn.dataset.nameboton) : "", // ejemplo: "abre tu cuenta",
                            ubicacion: "banner principal"
                        })
                    })
                }



            });


        }
    }

    try {
        window.addEventListener("load", () => {
            funcionalidadesBtnDataLayer();
        });
    } catch (error) {
        console.log("Error en funcionalidadesBtnDataLayer()", error);

    }


</script>
